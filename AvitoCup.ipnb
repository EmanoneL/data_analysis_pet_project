list()\n\n    values = [1] * len(users)\n\n    sparse_matrix = csr_matrix((values, (rows, cols)), shape=(len(user_ids), len(item_ids)))\n\n    model = implicit.als.AlternatingLeastSquares(iterations=10, factors=60)\n    model.fit(sparse_matrix, )\n\n\n    user4pred = np.array([user_id_to_index[i] for i in user_to_pred])\n\n    recommendations, scores = model.recommend(user4pred, sparse_matrix[user4pred], N=40, filter_already_liked_items=True)\n\n    df_pred = pl.DataFrame(\n        {\n            'node': [\n                [index_to_item_id[i] for i in i] for i in recommendations.tolist()\n            ],\n             'cookie': list(user_to_pred),\n            'scores': scores.tolist()\n\n        }\n    )\n    df_pred = df_pred.explode(['node', 'scores'])\n    return df_pred","metadata":{"id":"118b73e6-c306-4ab8-8ae5-3f133a95cda5","trusted":true,"execution":{"iopub.status.busy":"2025-04-01T16:33:38.408295Z","iopub.execute_input":"2025-04-01T16:33:38.408706Z","iopub.status.idle":"2025-04-01T16:33:38.416314Z","shell.execute_reply.started":"2025-04-01T16:33:38.408661Z","shell.execute_reply":"2025-04-01T16:33:38.415340Z"}},"outputs":[],"execution_count":26},{"id":"7ec48ba7-af95-49a4-a8e3-f9d24e9a5b2d","cell_type":"code","source":"from scipy.sparse import csr_matrix\nimport numpy as np\nimport implicit\n\n\nusers = df_train[\"cookie\"]\nnodes = df_train[\"node\"]\neval_users = df_eval['cookie'].unique().to_list()\n\ndf_pred = get_als_pred(users, nodes,eval_users )\n\n\n","metadata":{"scrolled":true,"id":"7ec48ba7-af95-49a4-a8e3-f9d24e9a5b2d","trusted":true,"execution":{"iopub.status.busy":"2025-04-01T16:33:41.639874Z","iopub.execute_input":"2025-04-01T16:33:41.640192Z","iopub.status.idle":"2025-04-01T16:35:41.902397Z","shell.execute_reply.started":"2025-04-01T16:33:41.640169Z","shell.execute_reply":"2025-04-01T16:35:41.901387Z"}},"outputs":[{"name":"stdout","text":"  (0, 1789)\t1\n  (0, 1793)\t1\n  (0, 1796)\t1\n  (0, 1806)\t8\n  (0, 46642)\t4\n  (0, 46644)\t1\n  (0, 631